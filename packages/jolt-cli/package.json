{
  "name": "jolt-cli",
  "version": "2.2.3",
  "description": "Build high-quality packages faster with Jolt.",
  "license": "MIT",
  "author": "Mark Miyashita <negativetwelve@gmail.com>",
  "homepage": "https://github.com/negativetwelve/jolt",
  "repository": "negativetwelve/jolt",
  "keywords": [
    "cli",
    "development",
    "dx",
    "jolt"
  ],
  "main": "build/jolt",
  "files": [
    "bin",
    "build"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "bin": {
    "jolt": "build/jolt.js"
  },
  "dependencies": {
    "jolt-config": "^2.1.0",
    "jolt-core": "*",
    "jolt-scripts": "*",
    "jolt-utils": "*",
    "update-notifier": "^2.2.0"
  },
  "scripts": {
    "build": "babel src --out-dir build --ignore '**/*.spec.js'",
    "clean": "rimraf build",
    "prepublishOnly": "yarn run clean && yarn run build"
